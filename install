cp 2.17.0.tar.gz /tmp
cd /tmp
tar -xzf 2.17.0.tar.gz
cd arb-2.17.0/
sudo apt-get update && apt-get install -y \
    autoconf \
    build-essential \
    cmake \
    wget \
    libtool \
    pkg-config \
    xz-utils \
    libgmp-dev \
    libmpfr-dev \
    libflint-dev \
    libflint-arb-dev libflint-arb-doc libflint-arb2 \
    libpng-dev libjpeg-dev
make install
rm -rf /arb-2.17.0
